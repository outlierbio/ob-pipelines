{
    "jobDefinitionName": "multiqc",
    "type": "container",
    "containerProperties": {
        "image": "outlierbio/multiqc",
        "vcpus": 2,
        "memory": 8000,
        "command": [
            "Ref::analysis_dir"
        ],
        "environment": [
            {
                "name": "SCRATCH_DIR",
                "value": "/scratch"
            }
        ],
        "volumes": [
            {
                "host": {
                    "sourcePath": "/mnt/scratch"
                },
                "name": "scratch"
            }
        ],
        "mountPoints": [
            {
                "containerPath": "/scratch",
                "readOnly": false,
                "sourceVolume": "scratch"
            }
        ]
    }
}